<plugin>

   <extension-point id="edu.toronto.cs.se.mmtf.models" name="MMTF model types" schema="schema/edu.toronto.cs.se.mmtf.models.exsd"/>
   <extension-point id="edu.toronto.cs.se.mmtf.relationships" name="MMTF model relationship types" schema="schema/edu.toronto.cs.se.mmtf.relationships.exsd"/>
   <extension-point id="edu.toronto.cs.se.mmtf.editors" name="MMTF editor types" schema="schema/edu.toronto.cs.se.mmtf.editors.exsd"/>
   <extension-point id="edu.toronto.cs.se.mmtf.operators" name="MMTF operator types" schema="schema/edu.toronto.cs.se.mmtf.operators.exsd"/>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://se.cs.toronto.edu/mmtf/Model"
            class="edu.toronto.cs.se.mmtf.mid.MidPackage"/>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.models">
      <modelType
            isAbstract="true">
         <extendibleType
               name="Model"
               uri="http://se.cs.toronto.edu/mmtf/Model">
         </extendibleType>
      </modelType>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://se.cs.toronto.edu/mmtf/ModelRel"
            class="edu.toronto.cs.se.mmtf.mid.relationship.RelationshipPackage"/>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.relationships">
      <relationshipType>
         <modelType>
            <extendibleType
                  name="ModelRel"
                  uri="http://se.cs.toronto.edu/mmtf/ModelRel">
            <superType
                  uri="http://se.cs.toronto.edu/mmtf/Model">
            </superType>
            </extendibleType>
         </modelType>
         <modelTypeEndpoint>
            <extendibleType
                  name="ModelEndpoint"
                  uri="http://se.cs.toronto.edu/mmtf/ModelRel-Model/ModelEndpoint">
            </extendibleType>
            <typeEndpoint
                  lowerBound="0"
                  upperBound="-1"
                  targetTypeUri="http://se.cs.toronto.edu/mmtf/Model">
            </typeEndpoint>
            <modelElemType
                  category="ENTITY"
                  classLiteral="ModelElementEntityWildcard">
                <extendibleType
                     name="EntityElem"
                     uri="http://se.cs.toronto.edu/mmtf/Model/EntityElem">
                </extendibleType>
            </modelElemType>
            <modelElemType
                  category="RELATIONSHIP"
                  classLiteral="ModelElementEntityWildcard/modelElementRelationshipWildcard">
                <extendibleType
                     name="RelElem"
                     uri="http://se.cs.toronto.edu/mmtf/Model/RelElem">
                </extendibleType>
            </modelElemType>
         </modelTypeEndpoint>
         <linkType>
            <extendibleType
                  name="Link"
                  uri="http://se.cs.toronto.edu/mmtf/ModelRel/Link">
            </extendibleType>
            <modelElemTypeEndpoint>
               <extendibleType
                     name="EntityElemEndpoint"
                     uri="http://se.cs.toronto.edu/mmtf/ModelRel/Link-EntityElem/EntityElemEndpoint">
               </extendibleType>
               <typeEndpoint
                     lowerBound="0"
                     upperBound="-1"
                     targetTypeUri="http://se.cs.toronto.edu/mmtf/Model/EntityElem">
               </typeEndpoint>
            </modelElemTypeEndpoint>
            <modelElemTypeEndpoint>
               <extendibleType
                     name="RelElemEndpoint"
                     uri="http://se.cs.toronto.edu/mmtf/ModelRel/Link-RelElem/RelElemEndpoint">
               </extendibleType>
               <typeEndpoint
                     lowerBound="0"
                     upperBound="-1"
                     targetTypeUri="http://se.cs.toronto.edu/mmtf/Model/RelElem">
               </typeEndpoint>
            </modelElemTypeEndpoint>
         </linkType>
      </relationshipType>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://se.cs.toronto.edu/mmtf/MID"
            class="edu.toronto.cs.se.mmtf.mid.MidPackage"
            genModel="model/mid.genmodel"/>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.models">
      <modelType>
         <extendibleType
               name="MID"
               uri="http://se.cs.toronto.edu/mmtf/MID">
         </extendibleType>
      </modelType>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://se.cs.toronto.edu/mmtf/MID/Relationship"
            class="edu.toronto.cs.se.mmtf.mid.relationship.RelationshipPackage"
            genModel="model/mid.genmodel"/>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.models">
      <modelType>
         <extendibleType
               name="Relationship"
               uri="http://se.cs.toronto.edu/mmtf/MID/Relationship">
         </extendibleType>
      </modelType>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://se.cs.toronto.edu/mmtf/MID/Editor"
            class="edu.toronto.cs.se.mmtf.mid.editor.EditorPackage"
            genModel="model/mid.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://se.cs.toronto.edu/mmtf/MID/Operator"
            class="edu.toronto.cs.se.mmtf.mid.operator.OperatorPackage"
            genModel="model/mid.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://se.cs.toronto.edu/mmtf/MAVO"
            class="edu.toronto.cs.se.mmtf.mavo.MavoPackage"/>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.models">
      <modelType
            isAbstract="true">
         <extendibleType
               name="Model_MAVO"
               uri="http://se.cs.toronto.edu/mmtf/MAVO">
            <superType
                  uri="http://se.cs.toronto.edu/mmtf/Model">
            </superType>
         </extendibleType>
         <constraint
               implementation="edu.toronto.cs.se.mmtf.mavo.constraint.MAVOModelConstraint"
               language="JAVA">
         </constraint>
      </modelType>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://se.cs.toronto.edu/mmtf/MAVORefinementRel"
            class="edu.toronto.cs.se.mmtf.mid.relationship.RelationshipPackage"/>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.relationships">
      <relationshipType>
         <modelType>
            <extendibleType
                  name="MAVORefinementRel"
                  uri="http://se.cs.toronto.edu/mmtf/MAVORefinementRel">
            <superType
                  uri="http://se.cs.toronto.edu/mmtf/ModelRel">
            </superType>
            </extendibleType>
            <constraint
                  implementation="modelEndpoints-&gt;at(1).target.metatypeUri = modelEndpoints-&gt;at(2).target.metatypeUri and links-&gt;forAll(let origModelElemEndpoints : Sequence(mid::relationship::ModelElementEndpoint) = modelElemEndpoints-&gt;select(target.oclContainer().oclAsType(mid::Model).uri = oclContainer().oclContainer().oclAsType(mid::relationship::ModelRel).modelEndpoints-&gt;at(1).target.uri), refinedModelElemEndpoints : Sequence(mid::relationship::ModelElementEndpoint) = modelElemEndpoints-&gt;select(target.oclContainer().oclAsType(mid::Model).uri = oclContainer().oclContainer().oclAsType(mid::relationship::ModelRel).modelEndpoints-&gt;at(2).target.uri) in ((origModelElemEndpoints-&gt;size() = 1 and ((not origModelElemEndpoints-&gt;at(1).target.may implies refinedModelElemEndpoints-&gt;exists(not target.may)) and (not origModelElemEndpoints-&gt;at(1).target.set implies (refinedModelElemEndpoints-&gt;size() = 1 and refinedModelElemEndpoints-&gt;exists(not target.set))) and (not origModelElemEndpoints-&gt;at(1).target.var implies refinedModelElemEndpoints-&gt;forAll(not target.var)))) or (origModelElemEndpoints-&gt;size() = 1 and (origModelElemEndpoints-&gt;at(1).target.may and refinedModelElemEndpoints-&gt;isEmpty())) or (refinedModelElemEndpoints-&gt;size() = 1 and ((origModelElemEndpoints-&gt;exists(not target.may) implies not refinedModelElemEndpoints-&gt;at(1).target.may) and (origModelElemEndpoints-&gt;exists(not target.set) implies not refinedModelElemEndpoints-&gt;at(1).target.set) and (origModelElemEndpoints-&gt;exists(not target.var) implies (origModelElemEndpoints-&gt;one(not target.var) and not refinedModelElemEndpoints-&gt;at(1).target.var)))) or (refinedModelElemEndpoints-&gt;size() = 1 and (origModelElemEndpoints-&gt;isEmpty() and oclContainer().oclAsType(mid::relationship::ModelRel).modelEndpoints-&gt;at(1).target.oclAsType(mid::Model).inc))))"
                  language="OCL">
            </constraint>
         </modelType>
         <modelTypeEndpoint>
            <extendibleType
                  name="MAVOModelEndpoint"
                  uri="http://se.cs.toronto.edu/mmtf/MAVORefinementRel-MAVO/MAVOModelEndpoint">
            </extendibleType>
            <typeEndpoint
                  lowerBound="2"
                  upperBound="2"
                  targetTypeUri="http://se.cs.toronto.edu/mmtf/MAVO">
            </typeEndpoint>
            <modelElemType
                  category="ENTITY"
                  classLiteral="MAVOElement">
                <extendibleType
                     name="MAVOElement"
                     uri="http://se.cs.toronto.edu/mmtf/MAVO/MAVOElement">
                </extendibleType>
            </modelElemType>
            <modelElemType
                  category="RELATIONSHIP"
                  classLiteral="MAVOReference">
                <extendibleType
                     name="MAVOReference"
                     uri="http://se.cs.toronto.edu/mmtf/MAVO/MAVOReference">
                </extendibleType>
            </modelElemType>
         </modelTypeEndpoint>
         <linkType>
            <extendibleType
                  name="RefinementLink"
                  uri="http://se.cs.toronto.edu/mmtf/MAVORefinementRel/RefinementLink">
            </extendibleType>
            <modelElemTypeEndpoint>
               <extendibleType
                     name="MAVOElementEndpoint"
                     uri="http://se.cs.toronto.edu/mmtf/MAVORefinementRel/RefinementLink-MAVOElement/MAVOElementEndpoint">
               </extendibleType>
               <typeEndpoint
                     lowerBound="0"
                     upperBound="-1"
                     targetTypeUri="http://se.cs.toronto.edu/mmtf/MAVO/MAVOElement">
               </typeEndpoint>
            </modelElemTypeEndpoint>
            <modelElemTypeEndpoint>
               <extendibleType
                     name="MAVOReferenceEndpoint"
                     uri="http://se.cs.toronto.edu/mmtf/MAVORefinementRel/RefinementLink-MAVOReference/MAVOReferenceEndpoint">
               </extendibleType>
               <typeEndpoint
                     lowerBound="0"
                     upperBound="-1"
                     targetTypeUri="http://se.cs.toronto.edu/mmtf/MAVO/MAVOReference">
               </typeEndpoint>
            </modelElemTypeEndpoint>
         </linkType>
      </relationshipType>
   </extension>

   <extension
         point="org.eclipse.papyrus.uml.extensionpoints.UMLProfile">
      <profile
            description="MAVO support in UML models"
            iconpath="platform:/plugin/org.eclipse.uml2.uml.edit/icons/full/obj16/Profile.gif"
            name="MAVOProfile"
            path="platform:/plugin/edu.toronto.cs.se.mmtf/model/mavo.profile.uml"
            provider="Software Engineering Group, University of Toronto">
      </profile>
   </extension>

</plugin>

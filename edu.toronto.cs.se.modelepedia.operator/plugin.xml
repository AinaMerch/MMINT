<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://se.cs.toronto.edu/mmtf/MAVORefinementRel"
            class="edu.toronto.cs.se.mmtf.mid.relationship.RelationshipPackage"/>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.relationships">
      <relationshipType>
         <modelType
               constraint="links-&gt;forAll(let origModelElemEndpoints : Sequence(ModelElementEndpoint) = modelElemEndpoints-&gt;select(target.oclContainer().oclAsType(Model).uri = oclContainer().oclContainer().oclAsType(ModelRel).modelEndpoints-&gt;at(1).target.uri), refinedModelElemEndpoints : Sequence(ModelElementEndpoint) = modelElemEndpoints-&gt;select(target.oclContainer().oclAsType(Model).uri = oclContainer().oclContainer().oclAsType(ModelRel).modelEndpoints-&gt;at(2).target.uri) in ((origModelElemEndpoints-&gt;size() = 1 and ((not origModelElemEndpoints-&gt;at(1).target.may implies refinedModelElemEndpoints-&gt;exists(not target.may)) and (not origModelElemEndpoints-&gt;at(1).target.set implies (refinedModelElemEndpoints-&gt;size() = 1 and refinedModelElemEndpoints-&gt;exists(not target.set))) and (not origModelElemEndpoints-&gt;at(1).target.var implies refinedModelElemEndpoints-&gt;forAll(not target.var)))) or (origModelElemEndpoints-&gt;size() = 1 and (origModelElemEndpoints-&gt;at(1).target.may and refinedModelElemEndpoints-&gt;isEmpty())) or (refinedModelElemEndpoints-&gt;size() = 1 and ((origModelElemEndpoints-&gt;exists(not target.may) implies not refinedModelElemEndpoints-&gt;at(1).target.may) and (origModelElemEndpoints-&gt;exists(not target.set) implies not refinedModelElemEndpoints-&gt;at(1).target.set) and (origModelElemEndpoints-&gt;exists(not target.var) implies (origModelElemEndpoints-&gt;one(not target.var) and not refinedModelElemEndpoints-&gt;at(1).target.var)))) or (refinedModelElemEndpoints-&gt;size() = 1 and (origModelElemEndpoints-&gt;isEmpty() and oclContainer().oclAsType(ModelRel).modelEndpoints-&gt;at(1).target.inc))))">
            <extendibleType
                  name="MAVORefinementRel"
                  uri="http://se.cs.toronto.edu/mmtf/MAVORefinementRel"
                  supertypeUri="http://se.cs.toronto.edu/mmtf/ModelRel">
            </extendibleType>
         </modelType>
         <modelTypeEndpoint>
            <extendibleType
                  name="MAVOModelEndpoint"
                  uri="http://se.cs.toronto.edu/mmtf/MAVORefinementRel-Model/MAVOModelEndpoint">
            </extendibleType>
            <typeEndpoint
                  lowerBound="2"
                  upperBound="2"
                  targetTypeUri="http://se.cs.toronto.edu/mmtf/Model">
            </typeEndpoint>
            <modelElemType
                  category="ENTITY"
                  classLiteral="ModelElementEntityWildcard">
                <extendibleType
                     name="MAVOEntityElem"
                     uri="http://se.cs.toronto.edu/mmtf/Model/MAVOEntityElem">
                </extendibleType>
            </modelElemType>
            <modelElemType
                  category="RELATIONSHIP"
                  classLiteral="ModelElementEntityWildcard/modelElementRelationshipWildcard">
                <extendibleType
                     name="MAVORelElem"
                     uri="http://se.cs.toronto.edu/mmtf/Model/MAVORelElem">
                </extendibleType>
            </modelElemType>
         </modelTypeEndpoint>
         <linkType>
            <extendibleType
                  name="RefinementLink"
                  uri="http://se.cs.toronto.edu/mmtf/MAVORefinementRel/RefinementLink">
            </extendibleType>
            <modelElemTypeEndpoint>
               <extendibleType
                     name="MAVOEntityElemEndpoint"
                     uri="http://se.cs.toronto.edu/mmtf/MAVORefinementRel/RefinementLink-MAVOEntityElem/MAVOEntityElemEndpoint">
               </extendibleType>
               <typeEndpoint
                     lowerBound="0"
                     upperBound="-1"
                     targetTypeUri="http://se.cs.toronto.edu/mmtf/Model/MAVOEntityElem">
               </typeEndpoint>
            </modelElemTypeEndpoint>
            <modelElemTypeEndpoint>
               <extendibleType
                     name="MAVORelElemEndpoint"
                     uri="http://se.cs.toronto.edu/mmtf/MAVORefinementRel/RefinementLink-MAVORelElem/MAVORelElemEndpoint">
               </extendibleType>
               <typeEndpoint
                     lowerBound="0"
                     upperBound="-1"
                     targetTypeUri="http://se.cs.toronto.edu/mmtf/Model/MAVORelElem">
               </typeEndpoint>
            </modelElemTypeEndpoint>
         </linkType>
      </relationshipType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.operators">
      <operatorType
            class="edu.toronto.cs.se.modelepedia.operator.experiment.ExperimentDriver"
            isConversion="false">
         <extendibleType
               name="ExperimentDriver"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_ExperimentDriver">
         </extendibleType>
         <input>
            <parameter
                  isVararg="false"
                  name="modelType"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/Model">
            </parameter>
         </input>
         <output>
         </output>
      </operatorType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.operators">
      <operatorType
            class="edu.toronto.cs.se.modelepedia.operator.reasoning.AlloyChecker"
            isConversion="false">
         <extendibleType
               name="AlloyChecker"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_AlloyChecker">
         </extendibleType>
         <input>
            <parameter
                  isVararg="false"
                  name="anything"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/Model">
            </parameter>
         </input>
         <output>
         </output>
      </operatorType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.operators">
      <operatorType
            class="edu.toronto.cs.se.modelepedia.operator.match.ModelNameMatch"
            isConversion="false">
         <extendibleType
               name="ModelNameMatch"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_ModelNameMatch">
         </extendibleType>
         <input>
            <parameter
                  isVararg="true"
                  name="models"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/Model">
            </parameter>
         </input>
         <output>
            <parameter
                  isVararg="false"
                  name="nameMatch"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/ModelRel">
            </parameter>
         </output>
      </operatorType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.operators">
      <operatorType
            class="edu.toronto.cs.se.modelepedia.operator.diff.ModelDifference"
            isConversion="false">
         <extendibleType
               name="ModelDifference"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_ModelDifference">
         </extendibleType>
         <input>
            <parameter
                  isVararg="false"
                  name="src"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/Model">
            </parameter>
            <parameter
                  isVararg="false"
                  name="match"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/ModelRel">
            </parameter>
            <parameter
                  isVararg="false"
                  name="tgt"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/Model">
            </parameter>
         </input>
         <output>
            <parameter
                  isVararg="false"
                  name="difference"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/ModelRel">
            </parameter>
         </output>
      </operatorType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.operators">
      <operatorType
            class="edu.toronto.cs.se.modelepedia.operator.slice.ChangeImpact"
            isConversion="false">
         <extendibleType
               name="ChangeImpact"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_ChangeImpact">
         </extendibleType>
         <input>
            <parameter
                  isVararg="false"
                  name="original"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/Model">
            </parameter>
            <parameter
                  isVararg="false"
                  name="difference"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/ModelRel">
            </parameter>
            <parameter
                  isVararg="false"
                  name="modified"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/Model">
            </parameter>
            <parameter
                  isVararg="false"
                  name="trace"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/ModelRel">
            </parameter>
            <parameter
                  isVararg="false"
                  name="impacted"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/Model">
            </parameter>
         </input>
         <output>
            <parameter
                  isVararg="false"
                  name="changeImpact"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/ModelRel">
            </parameter>
         </output>
      </operatorType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.operators">
      <operatorType
            class="edu.toronto.cs.se.modelepedia.operator.propagate.ChangePropagation"
            isConversion="false">
         <extendibleType
               name="ChangePropagation"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_ChangePropagation">
         </extendibleType>
         <input>
            <parameter
                  isVararg="false"
                  name="original"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/Model">
            </parameter>
            <parameter
                  isVararg="false"
                  name="refinement"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/MAVORefinementRel">
            </parameter>
            <parameter
                  isVararg="false"
                  name="refined"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/Model">
            </parameter>
            <parameter
                  isVararg="false"
                  name="trace"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/ModelRel">
            </parameter>
            <parameter
                  isVararg="false"
                  name="related"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/Model">
            </parameter>
         </input>
         <output>
            <parameter
                  isVararg="false"
                  name="propagated"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/Model">
            </parameter>
            <parameter
                  isVararg="false"
                  name="propagatedRefinement"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/MAVORefinementRel">
            </parameter>
            <parameter
                  isVararg="false"
                  name="propagatedTrace"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/ModelRel">
            </parameter>
         </output>
      </operatorType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.operators">
      <operatorType
            class="edu.toronto.cs.se.modelepedia.operator.reasoning.Z3SMTSolver"
            isConversion="false">
         <extendibleType
               name="Z3SMTSolver"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_Z3SMTSolver">
         </extendibleType>
         <input>
            <parameter
                  isVararg="false"
                  name="anything"
                  modelTypeUri="http://se.cs.toronto.edu/mmtf/Model">
            </parameter>
         </input>
         <output>
         </output>
      </operatorType>
   </extension>

</plugin>

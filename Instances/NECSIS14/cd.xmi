<?xml version="1.0" encoding="ASCII"?>
<classdiagram:ClassDiagram
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:classdiagram="http://classdiagram/1.0"
    xsi:schemaLocation="http://classdiagram/1.0 ClassDiagram.ecore">
  <classes name="A">
    <attributes
        name="a"/>
  </classes>
  <classes name="B"
      parent="//@classes.0">
    <attributes
        name="b"/>
  </classes>
  <associations
      name="AB"
      source="//@classes.0"
      target="//@classes.1"
      upperBound="-1"/>
  <associations
      name="BA"
      source="//@classes.1"
      target="//@classes.0"
      upperBound="1"/>
</classdiagram:ClassDiagram>

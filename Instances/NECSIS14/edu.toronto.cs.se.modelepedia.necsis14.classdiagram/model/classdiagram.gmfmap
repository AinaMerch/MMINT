<?xml version="1.0" encoding="ASCII"?>
<gmfmap:Mapping
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="classdiagram.ecore#//ClassDiagram/classes"/>
    <ownedChild
        relatedDiagrams="//@diagram">
      <domainMetaElement
          href="classdiagram.ecore#//Class"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="classdiagram.gmfgraph#ClassLabel"/>
        <features
            href="classdiagram.ecore#//NamedElement/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="classdiagram.gmftool#//@palette/@tools.0/@tools.1"/>
      <diagramNode
          href="classdiagram.gmfgraph#Class"/>
      <children
          compartment="//@nodes.0/@ownedChild/@compartments.0">
        <containmentFeature
            href="classdiagram.ecore#//Class/attributes"/>
        <ownedChild>
          <domainMetaElement
              href="classdiagram.ecore#//Attribute"/>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping">
            <diagramLabel
                href="classdiagram.gmfgraph#AttributeLabel"/>
            <features
                href="classdiagram.ecore#//NamedElement/name"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="classdiagram.gmftool#//@palette/@tools.0/@tools.0"/>
          <diagramNode
              href="classdiagram.gmfgraph#Attribute"/>
        </ownedChild>
      </children>
      <compartments
          children="//@nodes.0/@ownedChild/@children.0">
        <compartment
            href="classdiagram.gmfgraph#ClassAttributesCompartment"/>
      </compartments>
    </ownedChild>
  </nodes>
  <links>
    <domainMetaElement
        href="classdiagram.ecore#//Association"/>
    <labelMappings
        xsi:type="gmfmap:FeatureLabelMapping">
      <diagramLabel
          href="classdiagram.gmfgraph#AssociationLabel"/>
      <features
          href="classdiagram.ecore#//NamedElement/name"/>
    </labelMappings>
    <containmentFeature
        href="classdiagram.ecore#//ClassDiagram/associations"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="classdiagram.gmftool#//@palette/@tools.1/@tools.0"/>
    <diagramLink
        href="classdiagram.gmfgraph#Association"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="classdiagram.ecore#//Association/source"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="classdiagram.ecore#//Association/target"/>
  </links>
  <links>
    <labelMappings
        xsi:type="gmfmap:DesignLabelMapping"
        readOnly="true">
      <diagramLabel
          href="classdiagram.gmfgraph#ClassParentExternalLabel"/>
    </labelMappings>
    <tool
        xsi:type="gmftool:CreationTool"
        href="classdiagram.gmftool#//@palette/@tools.1/@tools.1"/>
    <diagramLink
        href="classdiagram.gmfgraph#ClassParent"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="classdiagram.ecore#//Class/parent"/>
  </links>
  <diagram>
    <diagramCanvas
        href="classdiagram.gmfgraph#classdiagram"/>
    <domainModel
        href="classdiagram.ecore#/"/>
    <domainMetaElement
        href="classdiagram.ecore#//ClassDiagram"/>
    <palette
        href="classdiagram.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>

[comment encoding = UTF-8 /]
[module EcoreMAVOToSMTLIB_M2T('http://www.eclipse.org/emf/2002/Ecore', 'http://se.cs.toronto.edu/mmint/MAVO')/]
[import edu::toronto::cs::se::modelepedia::z3::mavo::EcoreMAVOToSMTLIBUtils_M2T /]

[template public encodeMAVOModel(mavoModel : MAVOModel, mavoModelName : String, mayOnly : Boolean)]
	[comment @main /]
[createIdsFromNames(mavoModel)/]
	[file (mavoModelName + '.smt2', false, 'UTF-8')]
		[if (mayOnly)]
[encodeModelMayOnly(mavoModel)/]
[encodeMAVOConstraintsMayOnly(mavoModel)/]
		[else]
[encodeModel(mavoModel)/]
[encodeMAVOConstraints(mavoModel)/]
		[/if]
	[/file]	
[/template]

<plugin>

   <extension-point id="edu.toronto.cs.se.mmint.models" name="MMINT model types" schema="schema/edu.toronto.cs.se.mmint.models.exsd"/>
   <extension-point id="edu.toronto.cs.se.mmint.relationships" name="MMINT model relationship types" schema="schema/edu.toronto.cs.se.mmint.relationships.exsd"/>
   <extension-point id="edu.toronto.cs.se.mmint.editors" name="MMINT editor types" schema="schema/edu.toronto.cs.se.mmint.editors.exsd"/>
   <extension-point id="edu.toronto.cs.se.mmint.operators" name="MMINT operator types" schema="schema/edu.toronto.cs.se.mmint.operators.exsd"/>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated mid -->
      <package
            uri="http://se.cs.toronto.edu/mmint/MID"
            class="edu.toronto.cs.se.mmint.mid.MidPackage"
            genModel="model/mid.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated mid -->
      <package
            uri="http://se.cs.toronto.edu/mmint/MAVO"
            class="edu.toronto.cs.se.mmint.mavo.MavoPackage"
            genModel="model/mid.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://se.cs.toronto.edu/mmint/Model"
            class="edu.toronto.cs.se.mmint.mid.MidPackage"/>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.models">
      <modelType
            isAbstract="true">
         <type
               name="Model"
               uri="http://se.cs.toronto.edu/mmint/Model">
         </type>
      </modelType>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://se.cs.toronto.edu/mmint/ModelRel"
            class="edu.toronto.cs.se.mmint.mid.relationship.RelationshipPackage"/>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.relationships">
      <relationshipType>
         <modelType>
            <type
                  name="ModelRel"
                  uri="http://se.cs.toronto.edu/mmint/ModelRel">
            <superType
                  uri="http://se.cs.toronto.edu/mmint/Model">
            </superType>
            </type>
         </modelType>
         <modelTypeEndpoint>
            <type
                  name="ModelEndpoint"
                  uri="http://se.cs.toronto.edu/mmint/ModelRel-Model/ModelEndpoint">
            </type>
            <typeEndpoint
                  lowerBound="0"
                  upperBound="-1"
                  targetTypeUri="http://se.cs.toronto.edu/mmint/Model">
            </typeEndpoint>
            <modelElemType>
                <type
                     name="ModelElement"
                     uri="http://se.cs.toronto.edu/mmint/Model#//ModelElement">
                </type>
            </modelElemType>
         </modelTypeEndpoint>
         <linkType>
            <type
                  name="Link"
                  uri="http://se.cs.toronto.edu/mmint/ModelRel/Link">
            </type>
            <modelElemTypeEndpoint>
               <type
                     name="ModelElemEndpoint"
                     uri="http://se.cs.toronto.edu/mmint/ModelRel/Link-ModelElem/ModelElemEndpoint">
               </type>
               <typeEndpoint
                     lowerBound="0"
                     upperBound="-1"
                     targetTypeUri="http://se.cs.toronto.edu/mmint/Model#//ModelElement">
               </typeEndpoint>
            </modelElemTypeEndpoint>
         </linkType>
      </relationshipType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.models">
      <modelType>
         <type
               name="MID"
               uri="http://se.cs.toronto.edu/mmint/MID">
            <superType
                  uri="http://se.cs.toronto.edu/mmint/MAVO">
            </superType>
         </type>
      </modelType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.models">
      <modelType
            isAbstract="true">
         <type
               name="Model_MAVO"
               uri="http://se.cs.toronto.edu/mmint/MAVO">
            <superType
                  uri="http://se.cs.toronto.edu/mmint/Model">
            </superType>
         </type>
         <constraint
               implementation="edu.toronto.cs.se.mmint.mavo.constraint.MAVOModelConstraint"
               language="JAVA">
         </constraint>
      </modelType>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://se.cs.toronto.edu/mmint/MAVORefinementRel"
            class="edu.toronto.cs.se.mmint.mid.relationship.RelationshipPackage"/>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.relationships">
      <relationshipType>
         <modelType>
            <type
                  name="MAVORefinementRel"
                  uri="http://se.cs.toronto.edu/mmint/MAVORefinementRel">
            <superType
                  uri="http://se.cs.toronto.edu/mmint/ModelRel">
            </superType>
            </type>
            <constraint
                  implementation="modelEndpoints-&gt;at(1).target.metatypeUri = modelEndpoints-&gt;at(2).target.metatypeUri and links-&gt;forAll(let origModelElemEndpoints : Sequence(mid::relationship::ModelElementEndpoint) = modelElemEndpoints-&gt;select(target.oclContainer().oclAsType(mid::Model).uri = oclContainer().oclContainer().oclAsType(mid::relationship::ModelRel).modelEndpoints-&gt;at(1).target.uri), refinedModelElemEndpoints : Sequence(mid::relationship::ModelElementEndpoint) = modelElemEndpoints-&gt;select(target.oclContainer().oclAsType(mid::Model).uri = oclContainer().oclContainer().oclAsType(mid::relationship::ModelRel).modelEndpoints-&gt;at(2).target.uri) in ((origModelElemEndpoints-&gt;size() = 1 and ((not origModelElemEndpoints-&gt;at(1).target.may implies refinedModelElemEndpoints-&gt;exists(not target.may)) and (not origModelElemEndpoints-&gt;at(1).target.set implies (refinedModelElemEndpoints-&gt;size() = 1 and refinedModelElemEndpoints-&gt;exists(not target.set))) and (not origModelElemEndpoints-&gt;at(1).target.var implies refinedModelElemEndpoints-&gt;forAll(not target.var)))) or (origModelElemEndpoints-&gt;size() = 1 and (origModelElemEndpoints-&gt;at(1).target.may and refinedModelElemEndpoints-&gt;isEmpty())) or (refinedModelElemEndpoints-&gt;size() = 1 and ((origModelElemEndpoints-&gt;exists(not target.may) implies not refinedModelElemEndpoints-&gt;at(1).target.may) and (origModelElemEndpoints-&gt;exists(not target.set) implies not refinedModelElemEndpoints-&gt;at(1).target.set) and (origModelElemEndpoints-&gt;exists(not target.var) implies (origModelElemEndpoints-&gt;one(not target.var) and not refinedModelElemEndpoints-&gt;at(1).target.var)))) or (refinedModelElemEndpoints-&gt;size() = 1 and (origModelElemEndpoints-&gt;isEmpty() and oclContainer().oclAsType(mid::relationship::ModelRel).modelEndpoints-&gt;at(1).target.oclAsType(mid::Model).inc))))"
                  language="OCL">
            </constraint>
         </modelType>
         <modelTypeEndpoint>
            <type
                  name="MAVOModelEndpoint"
                  uri="http://se.cs.toronto.edu/mmint/MAVORefinementRel-MAVO/MAVOModelEndpoint">
            </type>
            <typeEndpoint
                  lowerBound="2"
                  upperBound="2"
                  targetTypeUri="http://se.cs.toronto.edu/mmint/MAVO">
            </typeEndpoint>
            <modelElemType>
                <type
                     name="MAVOElement"
                     uri="http://se.cs.toronto.edu/mmint/MAVO#//MAVOElement">
                </type>
            </modelElemType>
         </modelTypeEndpoint>
         <linkType>
            <type
                  name="RefinementLink"
                  uri="http://se.cs.toronto.edu/mmint/MAVORefinementRel/RefinementLink">
            </type>
            <modelElemTypeEndpoint>
               <type
                     name="MAVOElementEndpoint"
                     uri="http://se.cs.toronto.edu/mmint/MAVORefinementRel/RefinementLink-MAVOElement/MAVOElementEndpoint">
               </type>
               <typeEndpoint
                     lowerBound="0"
                     upperBound="-1"
                     targetTypeUri="http://se.cs.toronto.edu/mmint/MAVO#//MAVOElement">
               </typeEndpoint>
            </modelElemTypeEndpoint>
         </linkType>
      </relationshipType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.operators">
      <operatorType>
         <type
               name="ModelRelTypeTransformation"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_ModelRelTypeTransformation"
               class="edu.toronto.cs.se.mmint.transformation.ModelRelTypeTransformation">
         </type>
         <input>
            <parameter
                  isVararg="false"
                  isType="true"
                  name="modelRelType"
                  modelTypeUri="http://se.cs.toronto.edu/mmint/ModelRel">
            </parameter>
            <parameter
                  isVararg="false"
                  name="srcModel"
                  modelTypeUri="http://se.cs.toronto.edu/mmint/Model">
            </parameter>
         </input>
         <output>
            <parameter
                  isVararg="false"
                  name="tgtModel"
                  modelTypeUri="http://se.cs.toronto.edu/mmint/Model">
            </parameter>
            <parameter
                  isVararg="false"
                  name="traceModelRel"
                  modelTypeUri="http://se.cs.toronto.edu/mmint/ModelRel">
            </parameter>
         </output>
      </operatorType>
   </extension>

</plugin>

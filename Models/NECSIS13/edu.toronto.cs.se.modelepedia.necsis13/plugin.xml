<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://se.cs.toronto.edu/modelepedia/Java"
            class="edu.toronto.cs.se.mmtf.mid.MidPackage"/>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.models">
      <modelType>
         <extendibleType
               name="Java"
               uri="http://se.cs.toronto.edu/modelepedia/Java">
         </extendibleType>
      </modelType>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://se.cs.toronto.edu/modelepedia/WellFormedJava"
            class="edu.toronto.cs.se.mmtf.mid.relationship.RelationshipPackage"/>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.relationships">
      <relationshipType>
         <modelType>
            <extendibleType
                  name="WellFormedJava"
                  uri="http://se.cs.toronto.edu/modelepedia/WellFormedJava">
            </extendibleType>
         </modelType>
         <modelTypeEndpoint>
            <extendibleType
                  name="JavaEndpoint"
                  uri="http://se.cs.toronto.edu/modelepedia/WellFormedJava-Java/JavaEndpoint">
            </extendibleType>
            <typeEndpoint
                  lowerBound="1"
                  upperBound="1"
                  targetTypeUri="http://se.cs.toronto.edu/modelepedia/Java">
            </typeEndpoint>
         </modelTypeEndpoint>
      </relationshipType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.operators">
      <operatorType
            class="edu.toronto.cs.se.modelepedia.necsis13.operator.Dummy"
            isConversion="false">
         <extendibleType
               name="UMLMerge"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_UMLMerge">
         </extendibleType>
         <input>
            <parameter
                  isVararg="true"
                  name="models"
                  modelTypeUri="http://www.eclipse.org/uml2/4.0.0/UML">
            </parameter>
         </input>
         <output>
            <parameter
                  isVararg="false"
                  name="mergedModel"
                  modelTypeUri="http://www.eclipse.org/uml2/4.0.0/UML">
            </parameter>
         </output>
      </operatorType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.operators">
      <operatorType
            class="edu.toronto.cs.se.modelepedia.necsis13.operator.Dummy"
            isConversion="true">
         <extendibleType
               name="UML2Java"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_UML2Java">
         </extendibleType>
         <input>
            <parameter
                  isVararg="false"
                  name="uml"
                  modelTypeUri="http://www.eclipse.org/uml2/4.0.0/UML">
            </parameter>
         </input>
         <output>
            <parameter
                  isVararg="false"
                  name="java"
                  modelTypeUri="http://se.cs.toronto.edu/modelepedia/Java">
            </parameter>
         </output>
      </operatorType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.operators">
      <operatorType
            class="edu.toronto.cs.se.modelepedia.necsis13.operator.Dummy"
            isConversion="false">
         <extendibleType
               name="JavaOptCont"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_JavaOptCont">
         </extendibleType>
         <input>
            <parameter
                  isVararg="false"
                  name="java"
                  modelTypeUri="http://se.cs.toronto.edu/modelepedia/Java">
            </parameter>
         </input>
         <output>
            <parameter
                  isVararg="false"
                  name="optimizedJava"
                  modelTypeUri="http://se.cs.toronto.edu/modelepedia/Java">
            </parameter>
         </output>
      </operatorType>
   </extension>

</plugin>

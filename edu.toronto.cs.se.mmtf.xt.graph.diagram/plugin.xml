               class="edu.toronto.cs.se.mmtf.xt.graph.diagram.navigator.GraphNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="graph_diagram"/>
                  </and>
                  <instanceof value="edu.toronto.cs.se.mmtf.xt.graph.diagram.navigator.GraphAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>

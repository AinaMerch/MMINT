<plugin>

   <extension
         point="edu.toronto.cs.se.mmtf.operators">
      <operatorType>
         <type
               name="StateToArmy"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_StateToArmy"
               class="edu.toronto.cs.se.modelepedia.tutorial.operator.StateToArmy">
         </type>
         <input>
            <parameter
                  isVararg="false"
                  name="state"
                  modelTypeUri="http://se.cs.toronto.edu/modelepedia/State">
            </parameter>
         </input>
         <output>
            <parameter
                  isVararg="false"
                  name="army"
                  modelTypeUri="http://se.cs.toronto.edu/modelepedia/Army">
            </parameter>
         </output>
      </operatorType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.operators">
      <operatorType>
         <type
               name="StateToEconomy"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_StateToEconomy"
               class="edu.toronto.cs.se.modelepedia.tutorial.operator.StateToEconomy">
         </type>
         <input>
            <parameter
                  isVararg="false"
                  name="state"
                  modelTypeUri="http://se.cs.toronto.edu/modelepedia/State">
            </parameter>
         </input>
         <output>
            <parameter
                  isVararg="false"
                  name="economy"
                  modelTypeUri="http://se.cs.toronto.edu/modelepedia/Economy">
            </parameter>
         </output>
      </operatorType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmtf.operators">
      <operatorType>
         <type
               name="EconomyToArmy"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_EconomyToArmy"
               class="edu.toronto.cs.se.modelepedia.tutorial.operator.EconomyToArmy">
         </type>
         <input>
            <parameter
                  isVararg="false"
                  name="economy"
                  modelTypeUri="http://se.cs.toronto.edu/modelepedia/Economy">
            </parameter>
         </input>
         <output>
            <parameter
                  isVararg="false"
                  name="army"
                  modelTypeUri="http://se.cs.toronto.edu/modelepedia/Army">
            </parameter>
         </output>
      </operatorType>
   </extension>

</plugin>

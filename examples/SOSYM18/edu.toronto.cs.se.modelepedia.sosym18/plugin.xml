<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://se.cs.toronto.edu/modelepedia/ER"
            class="edu.toronto.cs.se.mmint.mid.MIDPackage"/>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.models">
      <modelType>
         <type
               name="ER"
               uri="http://se.cs.toronto.edu/modelepedia/ER">
         </type>
      </modelType>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://se.cs.toronto.edu/modelepedia/StateFlow"
            class="edu.toronto.cs.se.mmint.mid.MIDPackage"/>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.models">
      <modelType>
         <type
               name="StateFlow"
               uri="http://se.cs.toronto.edu/modelepedia/StateFlow">
         </type>
      </modelType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.operators">
      <operatorType>
         <type
               name="Rename"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_Rename"
               class="edu.toronto.cs.se.modelepedia.sosym18.operator.Dummy">
         </type>
         <inputs>
            <parameter>
               <type
                     name="orig"
                     uri="http://se.cs.toronto.edu/modelepedia/Operator_Rename-Model/orig">
               </type>
               <typeEndpoint
                     targetTypeUri="http://se.cs.toronto.edu/mmint/Model">
               </typeEndpoint>
            </parameter>
         </inputs>
         <outputs>
            <parameter>
               <type
                     name="renamed"
                     uri="http://se.cs.toronto.edu/modelepedia/Operator_Rename-Model/renamed">
               </type>
               <typeEndpoint
                     targetTypeUri="http://se.cs.toronto.edu/mmint/Model">
               </typeEndpoint>
            </parameter>
         </outputs>
      </operatorType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.operators">
      <operatorType>
         <type
               name="Rename"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_CDRename"
               class="edu.toronto.cs.se.modelepedia.sosym18.operator.Dummy">
            <superType
                  uri="http://se.cs.toronto.edu/modelepedia/Operator_Rename">
            </superType>
         </type>
         <inputs>
            <parameter>
               <type
                     name="orig"
                     uri="http://se.cs.toronto.edu/modelepedia/Operator_CDRename-CD/orig">
                  <superType
                        uri="http://se.cs.toronto.edu/modelepedia/Operator_Rename-Model/orig">
                  </superType>
               </type>
               <typeEndpoint
                     targetTypeUri="http://se.cs.toronto.edu/modelepedia/ClassDiagram">
               </typeEndpoint>
            </parameter>
         </inputs>
         <outputs>
            <parameter>
               <type
                     name="renamed"
                     uri="http://se.cs.toronto.edu/modelepedia/Operator_CDRename-CD/renamed">
                  <superType
                        uri="http://se.cs.toronto.edu/modelepedia/Operator_Rename-Model/renamed">
                  </superType>
               </type>
               <typeEndpoint
                     targetTypeUri="http://se.cs.toronto.edu/modelepedia/ClassDiagram">
               </typeEndpoint>
            </parameter>
         </outputs>
      </operatorType>
   </extension>

</plugin>

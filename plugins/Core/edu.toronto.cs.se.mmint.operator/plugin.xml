   <extension
         point="edu.toronto.cs.se.mmint.operators">
      <operatorType>
         <type
               name="ModelRelComposition"
               uri="http://se.cs.toronto.edu/mmint/Operator_ModelRelComposition"
               class="edu.toronto.cs.se.mmint.operator.merge.ModelRelComposition">
         </type>
         <constraint
               implementation="edu.toronto.cs.se.mmint.operator.merge.ModelRelComposition$InputConstraint"
               language="JAVA">
         </constraint>
         <inputs>
            <parameter>
	           <type
	                 name="rel1">
	           </type>
	           <typeEndpoint
	                 targetTypeUri="http://se.cs.toronto.edu/mmint/ModelRel">
	           </typeEndpoint>
            </parameter>
            <parameter>
	           <type
	                 name="rel2">
	           </type>
	           <typeEndpoint
	                 targetTypeUri="http://se.cs.toronto.edu/mmint/ModelRel">
	           </typeEndpoint>
            </parameter>
         </inputs>
         <outputs>
            <parameter>
	           <type
	                 name="composition">
	           </type>
	           <typeEndpoint
	                 targetTypeUri="http://se.cs.toronto.edu/mmint/ModelRel">
	           </typeEndpoint>
               <endpointConstraint
                     parameterName="rel1"
                     endpointIndex="0">
               </endpointConstraint>
               <endpointConstraint
                     parameterName="rel2"
                     endpointIndex="1">
               </endpointConstraint>
            </parameter>
         </outputs>
      </operatorType>
   </extension>

   <extension

<plugin>

   <extension
         point="edu.toronto.cs.se.mmint.operators">
      <operatorType>
         <type
               name="PetriNetSimulate"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_PetriNetSimulate"
               class="edu.toronto.cs.se.modelepedia.petrinet.operator.PetriNetSimulate">
         </type>
         <inputs>
            <parameter>
	           <type
	                 name="petriNet"
	                 uri="">
	           </type>
	           <typeEndpoint
	                 lowerBound="1"
	                 upperBound="1"
	                 targetTypeUri="http://se.cs.toronto.edu/modelepedia/PetriNet">
	           </typeEndpoint>
            </parameter>
         </inputs>
      </operatorType>
   </extension>

</plugin>

<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated gsn -->
      <package
            uri="http://www.cs.toronto.edu/~nlsfung/gsn"
            class="edu.toronto.cs.se.nlsfung.gsn.GSNPackage"
            genModel="model/gsn.genmodel"/>
   </extension>
   <extension
         point="edu.toronto.cs.se.mmint.models">
      <modelType>
         <type
               name="GSN"
               uri="http://www.cs.toronto.edu/~nlsfung/gsn">
         </type>
      </modelType>
   </extension>
   <extension
         point="org.eclipse.emf.ecore.generated_package">
      <package
            class="edu.toronto.cs.se.mmint.mid.relationship.RelationshipPackage"
            uri="http://www.cs.toronto.edu/~nlsfung/GSNRel">
      </package>
   </extension>
   <extension
         point="edu.toronto.cs.se.mmint.relationships">
      <relationshipType>
         <modelType>
            <type
                  name="GSNRel"
                  uri="http://www.cs.toronto.edu/~nlsfung/GSNRel">
            </type>
         </modelType>
         <modelTypeEndpoint>
            <type
                  name="GSNEndpoint"
                  uri="http://www.cs.toronto.edu/~nlsfung/GSNRel-GSN/GSNEndpoint">
            </type>
            <typeEndpoint
                  lowerBound="0"
                  targetTypeUri="http://www.cs.toronto.edu/~nlsfung/gsn"
                  upperBound="-1">
            </typeEndpoint>
         </modelTypeEndpoint>
      </relationshipType>
   </extension>

</plugin>
